{
  "name": "web-input-readline-hotkeys-extension",
  "version": "0.1.0",
  "description": "allow readline-style on select text input boxes",
  "scripts": {
    "dev": "cross-env NODE_ENV=development pnpm run build:webpack",
    "prod": "cross-env NODE_ENV=production pnpm run build:webpack",
    "build:webpack": "webpack -r ts-node/register -c webpack.config.ts",
    "lint": "run-p lint:js lint:css lint:misc",
    "lint:fix": "run-p lint:js:fix lint:css:fix lint:misc:fix",
    "lint:js": "eslint . --ext .ts --ext .tsx",
    "lint:js:fix": "eslint . --ext .ts --ext .tsx --fix",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "stylelint --fix \"**/*.css\"",
    "lint:misc": "prettier --check .",
    "lint:misc:fix": "prettier --write ."
  },
  "keywords": [
    "readline",
    "chrome",
    "chrome-extension",
    "firefox",
    "firefox-extension",
    "hotkeys",
    "extension"
  ],
  "author": "Edwin Kofler",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.4",
    "@types/chrome": "0.0.104",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/cssnano": "^4.0.0",
    "@types/duplicate-package-checker-webpack-plugin": "^2.1.0",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^13.13.0",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.12",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "cssnano": "^4.1.10",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "eslint-plugin-standard": "^4.0.1",
    "esm": "^3.2.25",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.4",
    "style-loader": "^1.1.4",
    "stylelint": "^13.3.2",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^7.0.0",
    "ts-node": "^8.8.2",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "typescript-plugin-css-modules": "^2.2.0",
    "url-loader": "^4.1.0",
    "web-ext-types": "^3.2.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "open-color": "^1.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}

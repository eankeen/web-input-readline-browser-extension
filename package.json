{
  "name": "text-input-readline-browser-extension",
  "version": "0.1.0",
  "description": "allow readline-style on select text input boxes",
  "scripts": {
    "dev": "cross-env NODE_ENV=development pnpm run build:webpack",
    "prod": "cross-env NODE_ENV=production pnpm run build:webpack",
    "build:rollup": "cross-env NODE_ENV=development rollup -c rollup.config.js",
    "build:webpack": "cross-env NODE_ENV=development webpack -r esm"
  },
  "keywords": [
    "readline",
    "chrome",
    "chrome-extension",
    "extension"
  ],
  "author": "Edwin Kofler",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/chrome": "0.0.104",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "esm": "^3.2.25",
    "mini-css-extract-plugin": "^0.9.0",
    "rollup": "^2.6.1",
    "rollup-plugin-terser": "^5.3.0",
    "style-loader": "^1.1.4",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
